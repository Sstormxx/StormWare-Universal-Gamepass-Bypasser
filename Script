local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("StormWare Gamepass Byapsser", "Synapse")

local TestTab = Window:NewTab("main")
local Section = TestTab:NewSection("Bypass")

Section:NewToggle("speed", "speed", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 115
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

Section:NewToggle("JumpPower", "Jump", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 115
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

Section:NewButton("Universal Gamepass Bypasser", "116 games", function()
